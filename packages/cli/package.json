  "homepage": "https://github.com/zhouhoujun/type-task#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "minimist": "^1.2.0",
    "interpret": "^1.1.0",
    "liftoff": "^2.5.0",
    "through2": "^2.0.1",
    "ts-node": "^7.0.0",
    "tsconfig-paths": "^3.5.0",
    "tslib": "^1.9.2",
