{
  "name": "type-task",
  "compilerOptions": {
    "importHelpers": true,
    "baseUrl": ".",
    "paths": {
      "@taskfr/core": [
        "packages/core/src"
      ],
      "@taskfr/node": [
        "packages/node/src"
      ],
      "@taskfr/build": [
        "packages/build/src"
      ],
      "@taskfr/pipes": [
        "packages/pipes/src"
      ],
      "@taskfr/pack": [
        "packages/pack/src"
      ],
      "@taskfr/platform-server": [
        "packages/platform-server/src"
      ]
    },
    "module": "commonjs",
    "target":"es5",
    "lib": ["dom", "es2015", "es2017"],
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "removeComments": false,
    "noLib": false,
    "jsx":"preserve",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "pretty": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "suppressImplicitAnyIndexErrors": true
  },
  "exclude": [
    "lib"
  ],
  "compileOnSave": false
}